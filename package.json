{
  "name": "world-datepicker",
  "version": "0.1.0",
  "description": "A Rect datepicker for calendars from around the world",
  "main": "lib/index.js",
  "types": "lib/src/Datepicker.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:es6": "tsc src",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps",
    "build:js:watch": "npm run build:js -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "coverage": "jest --config jest.config.js --coverage",
    "docs": "npm run docs:build && npm run docs:start",
    "docs:build": "rimraf docs && build-storybook -c ./tools/storybook -o docs",
    "docs:start": "start-storybook -p 6006 -c ./tools/storybook",
    "format": "npx eslint src/**/*.ts test/**/*.ts --fix",
    "lint": "npx eslint src/**/*.ts test/**/*.ts",
    "prestart": "npm run build",
    "start": "node server.js",
    "test": "jest --config jest.config.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "keywords": [
    "calendar",
    "coptic",
    "datepicker",
    "discworld",
    "ethiopian",
    "gregorian",
    "hebrew",
    "islamic",
    "julian",
    "mayan",
    "nanakshahi",
    "nepali",
    "persian",
    "taiwanese",
    "thai",
    "umm al-qura"
  ],
  "author": "Keith Wood",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kbwood/world-datepicker.git"
  },
  "files": ["lib/**/*"],
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "world-calendars": "git://github.com/kbwood/world-calendars.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-controls": "^6.1.20",
    "@storybook/addon-docs": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^27.2.5",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.5",
    "rimraf": "^2.6.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
